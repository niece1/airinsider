{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "albertcht/invisible-recaptcha": "^1.9.5",
        "aws/aws-sdk-php": "^3.199",
        "bugsnag/bugsnag-laravel": "^2.22.1",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.0",
        "laravel/socialite": "^5.0",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1.31",
        "mews/purifier": "^3.3.5",
        "predis/predis": "^1.1.7",
        "realrashid/sweet-alert": "^4.0.0",
        "spatie/laravel-backup": "^7.6.0",
        "spatie/laravel-cookie-consent": "^3.1.1",
        "spatie/laravel-sitemap": "^6.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.10",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpstan/phpstan": "^0.12.90",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.8.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
